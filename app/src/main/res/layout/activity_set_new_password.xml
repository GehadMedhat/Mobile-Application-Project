<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rtkkree9loar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/white_rectangle">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/rgbkz6k1gjao"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_marginTop="34dp"
                android:layout_marginBottom="13dp"
                android:layout_marginStart="30dp"
                android:src="@drawable/back_arrow"
                android:scaleType="fitXY"
                />
            <TextView
                android:id="@+id/r7e74mqdkfhe"
                android:textColor="#000000"
                android:textSize="31sp"
                android:textStyle="bold"
                android:layout_marginBottom="14dp"
                android:layout_marginStart="41dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Set New Password"
                />
            <LinearLayout
                android:id="@+id/r8cw9i1z0ygp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingRight="7dp"
                android:layout_marginBottom="25dp"
                android:layout_marginHorizontal="40dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rifyd9vfwi3d"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Create an unique password."
                    />
                <ImageView
                    android:id="@+id/loginImage"
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/newpassword"
                    android:scaleType="fitCenter"
                    android:layout_marginBottom="36dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/ru22fsc2bm7i"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="2dp"
                android:layout_marginBottom="29dp"
                android:layout_marginHorizontal="42dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r8ynxk983ecj"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginBottom="9dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="New Password"
                    />
                <LinearLayout
                    android:id="@+id/r2et0ydvs4o5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="@drawable/white_frame"
                    android:paddingRight="18dp"
                    android:orientation="horizontal">
                    <EditText
                        android:inputType="text"
                        android:hint="Create new password"
                        android:background="@null"
                        android:id="@+id/rehhqp4u7ksb"
                        android:textColor="#8A8C90"
                        android:textSize="14sp"
                        android:layout_marginEnd="4dp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:paddingVertical="16dp"
                        android:paddingLeft="18dp"
                        />
                    <ImageView
                        android:id="@+id/rti8b274681a"
                        android:layout_width="21dp"
                        android:layout_height="21dp"
                        android:src="@drawable/eye"
                        android:scaleType="fitXY"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rnwps7uk0rq"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="2dp"
                android:layout_marginBottom="18dp"
                android:layout_marginHorizontal="42dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r1a96y6k1ekn"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginBottom="9dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Confirm Password"
                    />
                <LinearLayout
                    android:id="@+id/r1mqdnuyds8t"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="@drawable/white_frame"
                    android:paddingRight="18dp"
                    android:orientation="horizontal">
                    <EditText
                        android:inputType="text"
                        android:hint="Re-enter password"
                        android:background="@null"
                        android:id="@+id/rqtdpu3cg5vd"
                        android:textColor="#8A8C90"
                        android:textSize="14sp"
                        android:layout_marginEnd="4dp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:paddingVertical="16dp"
                        android:paddingLeft="18dp"
                        />
                    <ImageView
                        android:id="@+id/rew5jukqcgn4"
                        android:layout_width="21dp"
                        android:layout_height="21dp"
                        android:src="@drawable/eye"
                        android:scaleType="fitXY"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r6yvcdehx5wg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/green_rectangle"
                android:paddingVertical="19dp"
                android:layout_marginBottom="71dp"
                android:layout_marginHorizontal="42dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rb4se4rdw8zk"
                    android:textColor="#FFFFFF"
                    android:textSize="23sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Reset Password"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rcbvopfqpspp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginBottom="67dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/r3xx4lw3g13z"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/rldumgthoik"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_marginEnd="4dp"
                        android:src="@drawable/back_arrow"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/rx5c7ew7ay8"
                        android:textColor="#000000"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Back to login?"
                        />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>