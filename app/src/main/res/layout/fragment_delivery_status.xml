            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/rs54njblc63i" android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="center_horizontal" android:layout_marginEnd="39dp" android:orientation="vertical">
            <ImageView android:id="@+id/rtl3qy7xujm" android:layout_width="24dp" android:layout_height="24dp" android:layout_marginBottom="4dp" android:src="@drawable/gray_diet" android:scaleType="fitXY"/>
            <LinearLayout android:id="@+id/rydisi92wm5" android:layout_width="wrap_content" android:layout_height="wrap_content" android:paddingBottom="1dp" android:orientation="vertical">
                <TextView android:id="@+id/rn0ppuy9pdpi" android:textColor="#737373" android:textSize="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Diet"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/r7zydsrg3nir" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1" android:gravity="center_horizontal" android:layout_marginEnd="34dp" android:orientation="vertical">
            <ImageView android:id="@+id/r5bwg98sfsik" android:layout_width="24dp" android:layout_height="24dp" android:layout_marginBottom="4dp" android:src="@drawable/gray_grocery" android:scaleType="fitXY"/>
