<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/r1vqwwdiintu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/white_rectangle">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/r91qtea1hswd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="34dp"
                android:layout_marginBottom="7dp"
                android:layout_marginStart="30dp"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/r0rvttdbbaec"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_marginBottom="13dp"
                    android:src="@drawable/back_arrow"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/rf5jbv28gy1f"
                    android:textColor="#000000"
                    android:textSize="31sp"
                    android:textStyle="bold"
                    android:layout_marginBottom="13dp"
                    android:layout_marginStart="11dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Verification"
                    />
                <TextView
                    android:id="@+id/ro2xt6mcz8w"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:layout_marginHorizontal="10dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Enter the code to continue."
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r7ehzmvdfz5j"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginBottom="17dp"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/loginImage"
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/verification"
                    android:scaleType="fitCenter"
                    android:layout_marginBottom="36dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r41hbttbpw0z"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginBottom="30dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/rozgsyo8bbg9"
                    android:layout_width="315dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="We sent a code to\nyouremail@outlook.com"
                    android:textColor="#000000"
                    android:textSize="21sp"
                    android:textStyle="bold" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rwqt7fq7r36p"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="29dp"
                android:layout_marginHorizontal="42dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/rgh5d99sz9ta"
                    android:layout_width="0dp"
                    android:layout_height="91dp"
                    android:layout_weight="1"
                    android:background="@drawable/white_frame"
                    android:layout_marginEnd="10dp"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ratw6a5giz7m"
                    android:layout_width="0dp"
                    android:layout_height="91dp"
                    android:layout_weight="1"
                    android:background="@drawable/white_frame"
                    android:layout_marginEnd="10dp"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rubcc8fa6rqe"
                    android:layout_width="0dp"
                    android:layout_height="91dp"
                    android:layout_weight="1"
                    android:background="@drawable/white_frame"
                    android:layout_marginEnd="10dp"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rozg26gx893j"
                    android:layout_width="0dp"
                    android:layout_height="91dp"
                    android:layout_weight="1"
                    android:background="@drawable/white_frame"
                    android:orientation="vertical">
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rngayjooszt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/green_rectangle"
                android:paddingVertical="19dp"
                android:layout_marginBottom="20dp"
                android:layout_marginHorizontal="46dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rde6klsprz6e"
                    android:textColor="#FFFFFF"
                    android:textSize="23sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Continue"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rvc0v22gumi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginBottom="66dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rx23q80yl3em"
                    android:textColor="#000000"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Didn't receive the code? "
                    />
                <TextView
                    android:id="@+id/rsendAgainBtn"
                    android:textColor="#C3E66E"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Send Again"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/razqss3h701q"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginBottom="67dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rolc2qeghal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/rgo75io1sqvj"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_marginEnd="4dp"
                        android:src="@drawable/back_arrow"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/rfkhgkclngl"
                        android:textColor="#000000"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Back to login?"
                        />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>